Algoritmo Fisbuzz
	
	para limite <- 1 Hasta 100 Con Paso 1 Hacer
		si limite % 3 == 0 & limite % 5 == 0 Entonces
			Imprimir 'FizzBuzz'
		FinSi
		si limite % 3 == 0 Entonces
			Imprimir 'Fizz'
		SiNo
			si limite % 5 == 0 Entonces
				imprimir 'Buzz'
			SiNo
				imprimir limite 
			FinSi
		FinSi
		
	FinPara
	
	
	
FinAlgoritmo
